(function&nbsp;imageLazy(window){
	var&nbsp;isLoadead&nbsp;=&nbsp;(document.readyState&nbsp;===&nbsp;&quot;complete&quot;);
	function&nbsp;lazyImages(){
		var&nbsp;obj;
		$(&quot;img.lazy&quot;).each(function&nbsp;changeSouce(){
